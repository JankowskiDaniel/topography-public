  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "def cyclic_MAE_test(output, target):\n",
    "    return min(abs(output-target), min(abs(output-target+1), abs(output-target-1)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "def save_results(outputs, targets, filename: str):\n",
    "    differences = []\n",
    "    for out, tar in zip(outputs, targets):\n",
    "        differences.append(cyclic_MAE_test(out, tar))\n",
    "    results_df = pd.DataFrame(list(zip(targets, outputs, differences)),\n",
    "                  columns =['epsilon', 'predicted', 'loss'])\n",
    "    results_df.to_csv(filename, index=False)\n",
    "    return results_df"
   ]
  },
